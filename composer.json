{
    "name": "daandelange/clavisverbum",
    "description": "A multilanguage taxonomy field for Kirby.",
    "homepage": "https://github.com/Daandelange/kirby-clavisverbum",
    "type": "kirby-plugin",
    "version": "0.2.1",
    "license": "MIT",
    "authors": [
        {
            "name": "Daan de Lange",
            "role": "Developer"
        }
    ],
    "keywords": [
        "kirby5",
        "kirby-plugin",
        "kirby-cms",
        "kirby-field",
        "keywords",
        "taxonomy",
        "multilanguage"
    ],
    "require": {
        "getkirby/composer-installer": "^1.1",
        "daandelange/helpers": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Daandelange\\Taxonomy\\": "src/"
        }
    },
    "scripts": {

    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    },
    "extra": {
        "installer-name": "clavisverbum",
        "kirby-plugin-path": "../"
    }
}
